              "@media (max-width: 768px)": {
                "padding-left": "0rem",
                "padding-right": "0rem"
              }
            },
            ".gallery-item, .gallery-row": {
              "&:nth-last-child(1)": {
                "padding-bottom": "0"
              }
            },
            "& when not (@paddingCard)": {
